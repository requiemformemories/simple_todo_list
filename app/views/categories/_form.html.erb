<% if @category.errors.any? %>
    <% @category.errors.full_messages.each do |msg| %>
    <div class="alert alert-danger" role="alert">
        <p><strong>Warning!</strong> <%= msg %></p>
    </div>
    <% end %>
<% end %>

<%= f.label :name, 'Name' %>
<%= f.text_field :name , :class => 'form-control'%>

<div class="col-md-6 col-md-offset-3 panel form-panel">
    <div class="panel-body">
        <%t = 'Edit a category' %>
        <%= render :partial => 'title', :locals => {:t => t} %>
        
        <%= form_for @category, :url => category_path(@category), :method => :patch do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
        
        <%= f.submit 'Update', :class => 'btn btn-default create-btn'%>
        
        <% end %>
    </div>
</div>